<UITemplate>
    <Using namespace="TemplateCompiler2Test"/>
    <Contents attr:x="x" attr:y="y" attr:z="{5 * 5}">
        <Div drag:create="() => new TestDragEvent()" mouse:down="HandleMouseDown($evt)" key:down="parentVal = 'set'">
            <Panel drag:drop="parentVal='dropped'" ctx:someContextValue="GetAttribute('y')">
                <ExpandedTemplate if="GetAttribute('x') == $someContextValue" sync:value="parentVal" onchange:value="ValueChanged()">
                    <override:title attr:x="{GetAttribute('z')}">
                        Hello
                    </override:title>
                </ExpandedTemplate>
            </Panel>
        </Div>
        <Group/>
<!--        <Group/>-->
<!--        <define:someslot>-->
<!--            <Text>Hello</Text>-->
<!--        </define:someslot>-->
    </Contents>
</UITemplate>