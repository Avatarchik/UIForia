<UITemplate>
    
    <Contents>

        <Div ctx:myVar="5 * 5">
            answer = {$myVar}
            
            <TemplateBindingTest_ContextVariable_SlotExposer>
                <OverrideSlot:some-slot>
                    
                    <Text id="text-el">
                        slot answer is = {$myVar * 2}
                    </Text>
                    
                </OverrideSlot:some-slot>
            </TemplateBindingTest_ContextVariable_SlotExposer>
            
        </Div>
        
    </Contents>
    
    <Contents id="slotexposer">
        <DefineSlot:some-slot>
            Default
        </DefineSlot:some-slot>
    </Contents>
    
</UITemplate>