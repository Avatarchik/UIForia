<UITemplate>

    <Contents id="repeat_count">

        <Repeat count="count">
            repeat me {$index}
        </Repeat>

    </Contents>

    <Contents id="repeat_list">

        <Repeat list="data">
            repeat me {$item}
        </Repeat>

    </Contents>

    <Contents id="repeat_list_key_fn">

        <Repeat list="data" keyFn="(e) => (long)e.x">
            repeat me {(long)$item.x}
        </Repeat>

    </Contents>

</UITemplate>

