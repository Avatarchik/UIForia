<UITemplate>

    <Style>
        
        style m {
            PreferredSize = 100px;
        }
        
    </Style>
    
    <Contents>

        <Div style="m" key:down="HandleKeyDown_NoParams()"/>
        
        <Div style="m" key:down="HandleKeyDown_EvtParam($evt)"/>
        
        <Div style="m" key:down="HandleKeyDown_MixedParams($evt, $evt.character)"/>
        
        <Div style="m" key:down="HandleKeyDown_NoEvtParam('string goes here', $evt.character)"/>
        
        <Div style="m" key:down="(evt) => SetValue(evt.character)"/>
        
        <Div style="m" key:down="() => SetValue($evt.character)"/>
        
    </Contents>
    
</UITemplate>