<?xml version="1.0" encoding="utf-8"?>
<UITemplate>

    <Style path="Klang/Seed/UIForia/BuildMenu/BuildMenu.style"/>

    <Contents style="build-item">

        <Repeat style="build-item-row" list="item.childItems">
            
            <KlangToolTip style="build-tooltip" text="$item.name">
                
                <BuildMenuItemDisplay item="$item"/>
                
            </KlangToolTip>
            
        </Repeat>

    </Contents>

</UITemplate>