<?xml version="1.0" encoding="utf-8"?>
<UITemplate>

    <Style>
        style container {
            FlexLayoutDirection = Row;
            PreferredWidth = 600;
            PreferredHeight = 600;
            OverflowY = Scroll;
            BackgroundColor = orange;
            TextFontSize = 76;
            TransformPositionX = 300;
            TransformPositionY = 300;
            LayoutBehavior = Ignored;
            TransformRotation = 0;
            TransformPivotX = 0;
            TransformPivotY = 0;
            Padding = 20px;
        }

        style input {
            PreferredSize = 1pca, 1cnt;
            BackgroundColor = white;
        }
        
        
    </Style>

    <Contents style="container">

        <Input style="input" textValue.bindTo="onValueChanged"/>
        
        <Router path="/">
            <Route path="/si"
        </Router>
