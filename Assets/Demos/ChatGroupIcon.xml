<?xml version="1.0" encoding="utf-8"?>
<UITemplate>

    <Style classPath="ChatGroupIcon+Styles"/>

    <Contents style="container">

        <Image style="chat-icon" src="{url(chatGroup.iconUrl)}"/>

        <Group style="unread-indicator">
            <Text style="unread-count">
                {chatGroup.unreadCount}
            </Text>
        </Group>

    </Contents>



</UITemplate>