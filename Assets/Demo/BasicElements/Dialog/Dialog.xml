<?xml version="1.0" encoding="utf-8"?>
<UITemplate>

    <Style path="Demo/BasicElements/Dialog/Dialog.style"/>
    <Style path="Demo/Theme.style" alias="theme"/>
    <Contents>
        <define:title style="dialog-title" />
        <Div style="dialog-buttons">
            <Div style="dialog-button color-grey" mouse:click="Cancel()">No</Div>
            <Div mouse:click="Confirm()" style="dialog-button" style.backgroundColor="confirmColor">Yes</Div>
        </Div>
        <define:description style="description" />
    </Contents>
</UITemplate>