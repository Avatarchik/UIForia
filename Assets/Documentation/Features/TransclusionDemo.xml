<?xml version="1.0" encoding="utf-8"?>
<UITemplate>

    <Style>

        style container {
         
            FlexLayoutDirection = Horizontal;
        }

        style transcluded {
            LayoutBehavior = TranscludeChildren;
            FlexLayoutDirection = Horizontal;
            Padding = 50px;
        }

        style child {
            BackgroundColor = red;
            PreferredSize = 100px;
        }
        
        style bleu {
            LayoutBehavior = Ignored;
            BackgroundColor = blue;
        }

    </Style>

    <Contents style="container">

        <Group style="transcluded">
            <Group style="transcluded">

                <Group style="child"/>
                <Group style="child"/>
                <Group style="child"/>
                <Group style="child"/>
                <Group style="child bleu"/>
                <Group style="child"/>
                
            </Group>
            
        </Group>
    </Contents>

</UITemplate>